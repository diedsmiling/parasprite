{
  "name": "parasprite",
  "version": "0.1.0",
  "description": "Define your GraphQL schema using chainable interface",
  "main": "./lib/parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "scripts": {
    "lint": "node_modules/.bin/eslint bin && node_modules/.bin/eslint src && node_modules/.bin/eslint test",
    "make": "bin/cake make --dest lib",
    "m": "npm run make",
    "watch": "bin/cake watch --dest lib",
    "w": "npm run watch",
    "test": "node_modules/.bin/ava",
    "t": "npm run test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "npm run test",
    "coverage": "npm run make && node_modules/.bin/nyc npm run test",
    "report": "npm run coverage && node_modules/.bin/nyc report --reporter=html npm run test"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "./test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-register": "^6.23.0",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "codecov": "1.0.1",
    "commander": "2.9.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "figures": "2.0.0",
    "glob": "7.1.1",
    "graphql": "0.9.x",
    "gulp-sourcemaps": "2.4.1",
    "gulp-util": "3.0.8",
    "junk": "2.0.0",
    "koa": "^2.0.1",
    "nyc": "^10.1.2",
    "ora": "1.1.0",
    "rimraf": "2.6.1",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "through2": "2.0.3",
    "vinyl-fs": "2.4.4",
    "vinyl-sourcemaps-apply": "0.2.1"
  },
  "peerDependencies": {
    "graphql": "0.9.x"
  },
  "dependencies": {
    "lodash.isfunction": "3.0.8"
  }
}
